{
  "name": "sabrewing",
  "version": "0.1.1",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./server": {
      "types": "./dist/server.d.ts",
      "import": "./dist/server.js",
      "require": "./dist/server.cjs"
    },
    "./vite-plugin-serverdollar": {
      "types": "./dist/vite-plugin-serverdollar.d.ts",
      "import": "./dist/vite-plugin-serverdollar.js",
      "require": "./dist/vite-plugin-serverdollar.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "build": "vite build --config vite.config.client.ts && vite build --config vite.config.server.ts"
  },
  "devDependencies": {
    "@types/jsdom": "^21.1.6",
    "@types/node": "^24.1.0",
    "puppeteer": "^24.15.0",
    "vitest": "^2.1.8"
  },
  "dependencies": {
    "vite": "^7.0.6",
    "jsdom": "^26.1.0",
    "@babel/parser": "^7.28.0",
    "@babel/types": "^7.28.2",
    "vite-plugin-dts": "^4.5.4",
    "@babel/traverse": "^7.28.0",
    "@babel/generator": "^7.28.0"
  }
}
